      "2018-08-18 16:39:04.644052\n"
     ]
    }
   ],
   "source": [
    "from datetime import datetime\n",
    "now = datetime.now()\n",
    "print(now)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2018-08-18 20:00:00\n"
     ]
    }
   ],
   "source": [
    "dt = datetime(2018, 8, 18, 20, 0, 0)\n",
    "print(dt)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1534593600.0\n"
     ]
    }
   ],
   "source": [
    "ts = dt.timestamp()\n",
    "print(ts)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2018-08-18 20:00:00\n"
     ]
    }
   ],
   "source": [
    "dt = datetime.fromtimestamp(ts)\n",
    "print(dt)"
   ]
